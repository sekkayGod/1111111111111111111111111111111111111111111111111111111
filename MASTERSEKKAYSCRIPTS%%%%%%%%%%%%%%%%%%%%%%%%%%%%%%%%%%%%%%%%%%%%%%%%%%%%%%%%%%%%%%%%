local function MOJWFX_fake_script() -- AntiAfkToggle.LocalScript 
	local script = Instance.new('LocalScript', AntiAfkToggle)

	_G.AntiAfk = false;
	
	local checked = false
	local a = script.Parent.Parent.Unchecked
	local b = script.Parent
	
	Tween = function(Obj, Time, Settings)	
		game:GetService("TweenService"):Create(Obj, TweenInfo.new(Time), Settings):Play()
	end
	
	b.MouseButton1Click:Connect(function()
		if checked == false then
			checked = true
			Tween(a, 0.2, {ImageTransparency = 1})
		elseif checked == true then
			checked = false
			Tween(a, 0.2, {ImageTransparency = 0})
		end
	end)
end
coroutine.wrap(MOJWFX_fake_script)()
local function HSUVNRF_fake_script() -- executeCounterBlox.Animations 
	local script = Instance.new('LocalScript', executeCounterBlox)

	local TweenService = game:GetService("TweenService")
	local TweenSpeed = 0.1
	local Info = TweenInfo.new(TweenSpeed, Enum.EasingStyle.Quad, Enum.EasingDirection.In, 0)
	
	local frame = script.Parent
	
	-- Button --
	
	script.Parent.MouseEnter:Connect(function()
		local Color = Color3.fromRGB(18, 18, 18)
		TweenService:Create(frame, Info, {BackgroundColor3 = Color}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		local ColorColor = Color3.fromRGB(35, 35, 35)
		TweenService:Create(frame, Info, {BackgroundColor3 = ColorColor}):Play()
	end)
	
	-- Text --
	
	script.Parent.MouseEnter:Connect(function()
		local Color = Color3.fromRGB(30, 30, 30)
		TweenService:Create(script.Parent.Text, Info, {TextColor3 = Color}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		local ColorColor = Color3.fromRGB(255, 255, 255)
		TweenService:Create(script.Parent.Text, Info, {TextColor3 = ColorColor}):Play()
	end)
end
